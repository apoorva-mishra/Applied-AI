   "source": [
    "n=int(input(\"Enter a number\"))\n",
    "\n",
    "def sumPdivisors(r):\n",
    "    div=[]\n",
    "    for i in range(r):\n",
    "        if(i>0 and n%i==0):\n",
    "            div.append(i)\n",
    "    \n",
    "    s=sum(div)\n",
    "    \n",
    "    return s\n",
    "x=sumPdivisors(n)\n",
    "print(\"sum of divisors\",x)"
   ]
